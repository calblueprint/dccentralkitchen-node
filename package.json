{
  "name": "dccentralkitchen-node",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:calblueprint/dccentralkitchen-node.git",
  "author": "anniero98 <anniero@berkeley.edu>",
  "license": "MIT",
  "scripts": {
    "generate-schema": "generate-airtable-schema && pretty-quick",
    "start": "babel-node index.js",
    "update-store-products": "babel-node scheduled-update.js"
  },
  "dependencies": {
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "airtable": "^0.8.1",
    "babel-eslint": "^10.1.0",
    "cors": "^2.8.5",
    "csv-parser": "^2.3.2",
    "dotenv-safe": "^8.2.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "express": "^4.17.1",
    "fs-extra": "^9.0.0",
    "googleapis": "39",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.1",
    "twilio": "^3.54.1"
  },
  "engines": {
    "node": "12.18.1"
  },
  "devDependencies": {
    "airtable-schema-generator": "^1.3.4",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "1.18.2",
    "pretty-quick": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "pretty-quick --staged",
      "eslint ./ --fix",
      "git add"
    ]
  },
  "airtable-schema-generator": {
    "output": "lib/airtable",
    "mode": "auto-headless",
    "overwrite": "false",
    "schemaMeta": {
      "Stores": {
        "lookupFields": [
          "Store Name"
        ]
      },
      "Products": {
        "lookupFields": [
          "Full Name"
        ]
      }
    }
  }
}
